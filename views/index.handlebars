<div  id= "first" class="container-fluid">
  <h1 id="header">CHOMP CHOMP BURGER!</h1>
  <img  id="burger"src="/assets/img/Burger Clipart 56948.jpg">
  <br>
   <h2 id= "headertwo"> IT'S CHOMPING TIME!</h2>
  </div>


 


<div id="second" class=container-fluid>
<h3>Burgers To TRY</h3>

<ul>
  {{#each burgers}}
  {{#unless devour}}
  {{> burgers/burger-block devoured=true}}
  {{/unless}}
  {{/each}}
</ul>

<h3>Burgers You CHOMPED</h3>

<ul>
  {{#each burgers}}
  {{#if devour}}
  {{> burgers/burger-block devoured=false}}
  {{/if}}
  {{/each}}
</ul>

<h3>Add A Burger to CHOMP</h3>
<form class="add-burger">

  <div class="form-group">
    <label for="ca">Burger Name:</label>
    <input type="text" id="ca" name="name">
    <input type="hidden" name="devour" value="0">
  </div>

  <div class="form-group">
    <label for="chomp">CHOMPED??!</label><br>
    <input type="radio" name="devour" value="1" id="devoured" checked> CHOMPED<br>
    <input type="radio" name="devour" value="0" id="noeat"> No CHOMP
  </div>

  <button type="submit">Add Burger</button>
</form>
</div>